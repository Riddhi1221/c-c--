#include<stdio.h>
#include<conio.h>
void hello()
{
	int a,b;
	printf("hello= ");
	scanf("%d",&a);
	printf("world= ");
	scanf("%d",&b);
	printf("%d\n",a);
	printf("%d\n",b);
}
void myFun()
{
	int n,i=1,f=1;
	printf("Enter number= ");
	scanf("%d",&n);
	while(i<=n)
	{
		f*=i;
		i++;
	}
	printf("The factorial of %d is %d\n",n,f);
}
void table(int n)
{
	int i=1;
	for(i=1;i<=10;i++)
	{
		printf("%d * %d = %d\n",n,i,n*i);
	}
	printf("\n");
}
void main()
{
	clrscr();
	hello();
	myFun();
	table(6);
	myFun();
	getch();
}